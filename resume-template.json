{
  "basics": {
    "name": "{{basics.name}}",
    "label": "{{basics.label}}",
    "imagePreview": "{{#basics.image}}<img src='{{basics.image}}' alt='profile-pic'/>{{/basics.image}}",
    "url": "{{basics.url}}",
    "summaryPreview": "{{#basics.summary}}<div class='summary-container'><div class='title'><h3>Professional Summary</h3></div><p class='summary'>{{basics.summary}}</p></div>{{/basics.summary}}"
  },
  "work": "{{#work.length}}<div class='container work-container'><div class='title'><h3>Professional History</h3></div>{{#work}}<h4 class='summary-position'>{{position}}</h4><h4>{{company}}</h4><h4>{{projectName}}</h4><h4>{{client}}</h4><h4>{{#city}}{{city}}. {{/city}}{{#startDate}}<span class='startDate'>{{startDate}}</span>{{/startDate}}<span class='endDate'> {{#inProgress}} - Present {{/inProgress}}{{^inProgress}}{{#endDate}} - {{endDate}}{{/endDate}}{{/inProgress}}</span></h4>{{#summary}}<p class='summary'>{{summary}}</p>{{/summary}}{{#skillsPreview}}{{name}}{{^last}} | {{/last}}{{/skillsPreview}} {{#technologiesPreview}}{{name}}{{^last}} | {{/last}}{{/technologiesPreview}}{{/work}}</div>{{/work.length}}",
  "education": "{{#education.length}}<div class='container education-container'><div class='title'><h3>Education</h3></div>{{#education}}<section class='item'>{{#studyType}}<h4>{{studyType}}</h4>{{/studyType}}{{#institution}}<h4>{{institution}}</h4>{{/institution}}<h4>{{#startDate}}<span class='startDate'>({{startDate}}</span>{{/startDate}}<span class='endDate'>{{#inProgress}} - Present {{/inProgress}}{{^inProgress}}{{#endDate}} - {{endDate}}{{/endDate}}{{/inProgress}})</span></h4></section>{{/education}}{{/education.length}}",
  "awards": "{{#awards.length}}<div class='container awards-container'><div class='title'><h3>Awards</h3></div>{{#awards}}<h4>{{title}}</h4>{{/awards}}</div>{{/awards.length}}",
  "skills": "{{#skills.length}}<div class='container skills-container'><div class='title'><h3>Skills</h3></div><section>{{#skills}}<h4>{{name}}</h4>{{/skills}}</section></div>{{/skills.length}}",
  "languages": "{{#languages.length}}<div class='container languages-container'><div class='title'><h3>Languages</h3></div>{{#languages}}<h4>{{language}} {{#fluency}}: {{fluency}} {{/fluency}}</h4>{{/languages}}</div>{{/languages.length}}"
}